pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
--game loop
function _init()
 map_setup()
 make_player()
 --txt
 text_setup()
 game_win=false
 game_over=false

end

function _update()
 if (not game_over) then
--txt
if (not active_text)then
  update_map()
  move_player()
  check_win_lose()
--run forest run
  run_player()
end 
else
 if (btnp(❎)) extcmd("reset")
 end

end

function _draw()
 cls()
 if (not game_over)then 
  draw_map()
  draw_player()
--run forest run
  run_player()
--txt
  draw_text()
 if(btn(❎)) show_inventory()
  else
   draw_win_lose()
  end

end


-->8
--map
function map_setup()
--timers-cat
 timer=0
 anim_time=30

--tile settings(flags)
 wall=0
 pretzel=1
 flag=2
 anim1=3
 anim2=4
 garbage=5
 lose=6
 win=7
 text=1
end

function update_map()
 if(timer<0) then
  cat_tiles()
  timer=anim_time
 end
 timer-=1
end

function draw_map()
--move the camera
 mapx=flr(player.x/16)*16
 mapy=flr(player.y/16)*16
 camera(mapx*8,mapy*8)
 
 
 map(0,0,0,0,128,64)
end
--check map tile (sprite flag) 
function is_tile(tile_type,x,y)
 tile=mget(x,y)
 has_flag=fget(tile,tile_type)
 return has_flag
end
--check if is a wall
function can_move(x,y)
 return not is_tile(wall,x,y)
 
end

--swap map tiles for pretzel
function swap_tile(x,y)
 tile=mget(x,y)
 mset(x,y,tile+1)
end

--animation for the cat tile

function unswap_tile(x,y)
 tile=mget(x,y)
 mset(x,y,tile-1)
end

function get_pretzel(x,y)
 player.pretzel+=1
 swap_tile(x,y)
 sfx(1) 
end

--for the gate
--pretzel tax
function pretzel_tax(x,y)
 player.pretzel-=1
 swap_tile(x,y)
 sfx(2)
end


-->8
--player & interact fc
function make_player()
 player={
 sp=1,
 x=1,
 y=14,
 pretzel=0,
 --aici incerc
 anim=0,
 }
end
--run forest run
--anim player
function run_player()
--aici incerc
if time()-player.anim>.1 then
 player.anim=time()
--aici termin
  player.sp+=1 
 if player.sp >3 then 
    player.sp=1
 end
end 
end

function draw_player()
 spr(player.sp,player.x*8,player.y*8)
--run forest run 
end

--move player
function move_player()
 newx=player.x
 newy=player.y
 if (btnp(⬅️)) newx-=1
 if (btnp(➡️)) newx+=1
 if (btnp(⬆️)) newy-=1
 if (btnp(⬇️)) newy+=1
--interact new x,y for pretzel
 interact(newx,newy)
 
 if (can_move(newx,newy)) then
  player.x=mid(0,newx,127)
  player.y=mid(0,newy,63)
 else
  sfx(0)  
 end
end

--interact with the pretzel
function interact(x,y)
--for text
 if (is_tile(text,x,y)) then
 active_text=get_text(x,y)
 end
--pretzel
 if (is_tile(pretzel,x,y)) then
  get_pretzel(x,y)
  elseif (is_tile(garbage,x,y) and
  player.pretzel>0) then
  pretzel_tax(x,y)
 end
end

--music background
music(0,0,36)

-->8
--how many pretzels do we have

function show_inventory()
 invx=mapx*8+40
 invy=mapy*8+8
 
--draw a rectangle for inventory
 rectfill(invx,invy,invx+48,invy+24,0)
 print("inventory", invx+7,invy+4,7)
 print("pretzels"..player.pretzel,invx+7,invy+15,9)
end

-->8
--animation cat

function cat_tiles()
 for x=mapx,mapx+15 do
  for y=mapy,mapy+15 do
   if (is_tile(anim1,x,y)) then
    swap_tile(x,y)
    sfx(3)
   elseif (is_tile(anim2,x,y)) then
    unswap_tile(x,y)
    sfx(3)
   end
  end
 end
end
-->8
--win or lose

function check_win_lose()
 if (is_tile(win,player.x,player.y)) then 
  game_win=true
  game_over=true
 elseif(is_tile(lose,player.x,player.y)) then
  game_win=false
  game_over=true 
 end
end

function draw_win_lose()
 camera()
 if (game_win) then
  print(" you win!🐱",37,64,7)
 else
  print("game over...",38,64,7)
 end
  print ("press ❎ to play",20,72,5)
end

-->8
--text?

function text_setup()
--table text
texts={}

add_text(4,14,"let's play!\nto see the inventory press x")
add_text(10,10,"tax!\nto pass you have to give\nthe crow a pretzel")
add_text(6,11,"after you collect pretzels\nyou can change the bags\nbe careful not to run out!")
add_text(11,6," beware!\n children!\n cats!")
add_text(14,11,"the cool guy!\nhe will give you a pretzel")
add_text(22,5,"when the lady is not\nlooking...\nrun!")

end

function add_text(x,y,message)
 texts[x+y*128]=message
end
function get_text(x,y)

 return texts[x+y*128]
end

function draw_text()
 if (active_text)then 
--the same way as i did
--for the inventory
 textx=mapx*8+4
 texty=mapy*8+48
 rectfill(textx,texty,textx+119,texty+31,7)
 print(active_text,textx+2,texty+2,1)
 print("🅾️ to close press z or c",text+4,texty+23,6)
 end
 if (btnp(🅾️)) active_text=nil
end 
